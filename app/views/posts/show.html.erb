<div class="post-wrapper">
	<div class="row">
			<div class="post-head">
			 <%= link_to (image_tag @post.image.url(:medium)), post_path(@post) %>
			 <%= @post.caption %>
			</div>
	</div>
	<div>
		<%= link_to "Cancel", posts_path %>
		<%= link_to "Edit", edit_post_path(@post)%>
	</div>
</div>





